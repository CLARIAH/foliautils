      <str xml:id="FP-0001.text.r1.l16">
        <t class="OCR" offset="866">s verständlich, weil die Dreifelderwirtschaft in der Art, wie sie für die</t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l16" type="str"/>
        </relation>
      </str>
      <str xml:id="FP-0001.text.r1.l17">
        <t class="OCR" offset="940">ältesten germanischen Verhältnisse in Frage kommen würde, keine</t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
