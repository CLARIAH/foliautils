      <str xml:id="FP-0001.text.r1.l5">
        <t class="OCR" offset="143">Betiebsweise. nung der Kaiserzeit behandelt werden soll, so ist</t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l5" type="str"/>
        </relation>
      </str>
      <str xml:id="FP-0001.text.r1.l6">
        <t class="OCR" offset="207">s der Rückblick, den wir auf die älteren Verhältnisse der römischen</t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l6" type="str"/>
        </relation>
      </str>
      <str xml:id="FP-0001.text.r1.l7">
        <t class="OCR" offset="275">Landwirtschaft werfen, ein cursorischer. Namentlich soll nicht ver<t-hbr>-</t-hbr></t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l7" type="str"/>
        </relation>
      </str>
      <str xml:id="FP-0001.text.r1.l8">
        <t class="OCR" offset="342">sucht werden, an der Hand der Ergebnisse der Ausgrabungen in den</t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l8" type="str"/>
        </relation>
      </str>
      <str xml:id="FP-0001.text.r1.l9">
