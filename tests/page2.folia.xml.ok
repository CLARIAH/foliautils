        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l27" type="str"/>
        </relation>
      </str>
      <str xml:id="FP-0001.text.r1.l28">
        <t class="OCR" offset="1666">3 Hehn, ulturpflanzen und Haustiere (vgl. Eileitung oben, S. 3f.).</t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
