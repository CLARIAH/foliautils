        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l14" type="str"/>
        </relation>
      </str>
      <str xml:id="FP-0001.text.r1.l15">
        <t class="OCR" offset="799">Dreifelderwirtschaft gebracht hätten, so ist dies schon deshalb un<t-hbr>-</t-hbr></t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
