        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l10" type="str"/>
        </relation>
      </str>
      <str xml:id="FP-0001.text.r1.l11">
        <t class="OCR" offset="531">o ältesten Besiedlungen an zu geben. In historischer Zeit bietet die</t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l11" type="str"/>
        </relation>
      </str>
      <str xml:id="FP-0001.text.r1.l12">
        <t class="OCR" offset="600">römische Landwirtschaft, wie sie uns die scriptores rei rusticae schil<t-hbr>-</t-hbr></t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l12" type="str"/>
        </relation>
      </str>
      <str xml:id="FP-0001.text.r1.l13">
        <t class="OCR" offset="671">dern, keinerlei besonders fremdartige Züge dar. Wenn früher gele<t-hbr>-</t-hbr></t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l13" type="str"/>
        </relation>
