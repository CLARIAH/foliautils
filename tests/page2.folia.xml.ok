      </str>
      <str xml:id="FP-0001.text.r1.l20">
        <t class="OCR" offset="1131">Schriftsteller aber kennen nur das, was Thünen „freie Wirtschaft“</t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l20" type="str"/>
        </relation>
      </str>
      <str xml:id="FP-0001.text.r1.l21">
        <t class="OCR" offset="1197">o nennen würde1). Von Fruchtfolge sprechen sie überhaupt nur gele- A22t</t>
        <relation format="text/page+xml" xlink:href="data/page/piros.xml" xlink:type="simple">
          <xref id="l21" type="str"/>
        </relation>
      </str>
